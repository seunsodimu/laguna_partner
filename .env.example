# Environment Configuration

# Application Environment
APP_DEBUG=false
APP_BASE_PATH=/

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_NAME=laguna_partner
DB_USER=root
DB_PASS=

# NetSuite Environment Selection
# Options: production, sandbox
NETSUITE_ENVIRONMENT=sandbox

# NetSuite Production Credentials
NETSUITE_PROD_ACCOUNT_ID=your-production-account-id
NETSUITE_PROD_CONSUMER_KEY=your-production-consumer-key
NETSUITE_PROD_CONSUMER_SECRET=your-production-consumer-secret
NETSUITE_PROD_TOKEN_ID=your-production-token-id
NETSUITE_PROD_TOKEN_SECRET=your-production-token-secret
NETSUITE_PROD_BASE_URL=https://your-account-id.suitetalk.api.netsuite.com

# NetSuite Sandbox Credentials
NETSUITE_SANDBOX_ACCOUNT_ID=your-sandbox-account-id
NETSUITE_SANDBOX_CONSUMER_KEY=your-sandbox-consumer-key
NETSUITE_SANDBOX_CONSUMER_SECRET=your-sandbox-consumer-secret
NETSUITE_SANDBOX_TOKEN_ID=your-sandbox-token-id
NETSUITE_SANDBOX_TOKEN_SECRET=your-sandbox-token-secret
NETSUITE_SANDBOX_BASE_URL=https://your-sandbox-account-id.suitetalk.api.netsuite.com

# Email Notifications
NOTIFICATION_FROM_EMAIL=noreply@lagunatools.com
NOTIFICATION_FROM_NAME="Laguna Partners Portal"
NOTIFICATION_TO_EMAILS=admin@lagunatools.com

# Email Provider (brevo or ses)
EMAIL_PROVIDER=brevo

# Brevo API Credentials
BREVO_API_KEY=your_brevo_api_key_here
BREVO_FROM_EMAIL=noreply@lagunatools.com
BREVO_FROM_NAME="Laguna Partners Portal"

# Amazon SES Credentials (if using SES)
# AWS_ACCESS_KEY_ID=your_aws_access_key_here
# AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
# AWS_SES_REGION=us-east-1
# SES_FROM_EMAIL=noreply@lagunatools.com
# SES_FROM_NAME="Laguna Partners Portal"

# Session Configuration
SESSION_SECURE=false

# Application URL
APP_URL=http://localhost